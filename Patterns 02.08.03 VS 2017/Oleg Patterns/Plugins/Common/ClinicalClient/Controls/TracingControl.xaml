             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Name="tracingControl"
             d:DesignWidth="365" d:DesignHeight="20"
             Loaded="tracingControl_Loaded">
    <Grid>
        <Border BorderBrush="LightGray" BorderThickness="1" Background="Beige" Width="362">
            <Canvas x:Name="drawingCanvas" Visibility="Visible" Width="360" ClipToBounds="True">
        
            </Canvas>
        </Border>
    </Grid>
</UserControl>
