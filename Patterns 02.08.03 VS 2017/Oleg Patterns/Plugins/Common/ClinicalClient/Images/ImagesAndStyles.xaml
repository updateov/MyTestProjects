    <ControlTemplate x:Key="IconBed">
        <Canvas Width="40" Height="25">
            <Path Data="F1M5.331,0L0,0 0,9.328 5.331,9.328z" Fill="#FF6B6768" Height="9.328" Canvas.Left="5.33" Canvas.Top="5.832" Width="5.331"/>
