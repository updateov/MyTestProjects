            <Path Data="F1M0,0L0,9.328 19.988,9.328 19.988,1.995C19.988,0.626,18.454,0,17.087,0z" Fill="#FF6B6768" Height="9.328" Canvas.Left="11.994" Canvas.Top="5.832" Width="19.988"/>
        </Canvas>
    </ControlTemplate>

    <ControlTemplate x:Key="IconHelpWhite">
