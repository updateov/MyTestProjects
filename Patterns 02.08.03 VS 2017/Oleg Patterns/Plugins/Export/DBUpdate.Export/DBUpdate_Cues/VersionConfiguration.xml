<?xml version="1.0" encoding="utf-8"?>
<!--Variables list
    #DBFile#
    #ArchiveDBFile#
	
 1. PrevTreeLastFolderExecuted & PrevTreeLastScriptExecuted are in order to prevent running newer tree without completeing the prev tree
	
-->
<Root>
  <Configurations>
    <CleanInstallatioHook Query="select 1 from Episodes LIMIT 1;"/> <!-- return 1 if this clean installation-->
    <DBConnections>
      <DBConnection Key="#DBFile#" Value="ExportDB.db" IsMainDB="true"/>
      <DBConnection Key="#ArchiveDBFile#" Value="ExportArchiveDB.db" IsMainDB="false"/>
    </DBConnections>
  </Configurations>
  <AppVersions CleanInstallationAppVersion="02.08.02" CleanInstallationFolderNumber="1" CleanInstallationScriptName="01_Script.sql" CreatedDate="2015-01-01">
    <AppVersion AppVersionFolder="02.08.02" BaseOnVersion="" PrevTreeLastFolderExecuted ="" PrevTreeLastScriptExecuted=""  CreatedDate="2015-01-01"/>
  </AppVersions>
</Root>
