            <Border Background="Black" Opacity="0.2" />
            <Border BorderBrush="Black" Opacity="0.7" BorderThickness="5" Margin="8,8,0,0" Width="405" Height="160" />
            <StackPanel VerticalAlignment="Center" Orientation="Vertical">
                <Border BorderBrush="{DynamicResource BrushSubMainBackground}" BorderThickness="1" 
					Background="{DynamicResource BrushSubMainBackground}" Width="404" Height="24"  >
                    <TextBlock x:Name="lblTitleSetFirstExamDialog" Text="Set as First Exam" VerticalAlignment="Stretch" Margin="4,7,0,0" FontWeight="Bold" TextAlignment="Left" Foreground="{DynamicResource brushColorHeading1}"  />
                </Border>
                <Border Width="404" Height="134" BorderThickness="4" BorderBrush="{DynamicResource BrushSubMainBackground}">
                    <Border Width="400" Height="130" BorderThickness="1" BorderBrush="{DynamicResource BrushSubMainBackground}" SnapsToDevicePixels="True" CornerRadius="0" Background="#FFb6d7e9">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <!--Question-->
                            <Image Source="images\question.png" Grid.Row="1" Stretch="None" VerticalAlignment="Stretch"/>
                            <TextBlock x:Name="lblMessage" Grid.Column="1" Grid.Row="1" Text="Confirm Set as first Exam the time 2008-02-15 12:25:36 PM?" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="5,35,5,0" />

                            <!--Buttons-->
                            <StackPanel Grid.Row="2" Grid.ColumnSpan="3" Orientation="Horizontal" HorizontalAlignment="Right">
                                <Button x:Name="btnOkFirstExam" Content="OK" MinWidth="85" MinHeight="25" Margin="1,1,1,7" />
                                <Button x:Name="btnCancelFirstExam" Content="Cancel" MinWidth="85" MinHeight="25" Margin="1,1,5,7" IsCancel="True" IsDefault="True" />
                            </StackPanel>
                        </Grid>
                    </Border>

                </Border>
            </StackPanel>
        </Grid>

    </Grid>

</UserControl>