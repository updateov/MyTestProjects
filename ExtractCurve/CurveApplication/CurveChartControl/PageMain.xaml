            <Setter Property="BorderThickness" Value="0,0,1,1" />
        </Style>
        <Style x:Key="ColumnCellFormatFixed" TargetType="Border">
            <Setter Property="SnapsToDevicePixels" Value="True"/>
            <Setter Property="Height" Value="20" />
            <Setter Property="Background" Value="{DynamicResource BrushHeaderColor}"/>
            <Setter Property="BorderBrush" Value="{DynamicResource BrushHeaderBorderColor}" />
            <Setter Property="BorderThickness" Value="0,0,1,1" />
            <Setter Property="Padding" Value="5,0,0,0" />
        </Style>
        <Style x:Key="ColumnHeaderFormat" TargetType="Border">
