        <Color x:Key="CellColorNoSelected">#FFe3eaed</Color>
        <Color x:Key="CellBorderColor">Gray</Color>
        <Color x:Key="HeaderColorOld">#FF485A68</Color>
        <Color x:Key="HeaderColor">#FFB6D7E9</Color>
        <Color x:Key="HeaderBorderColor">Gray</Color>
        <Color x:Key="MainBackground">#FF313131</Color>
        <Color x:Key="SubMainBackground">#FF4296C4</Color>
        <Color x:Key="SubMainBorderColor">DimGray</Color>
        <Color x:Key="TitleBackground">#FF4296C4</Color>
        <Color x:Key="DarkBevelBorderColor">#FF3D3C3C</Color>
        <Color x:Key="DarkBevelBackgroundColor">Gray</Color>
        <Color x:Key="LightBevelBorderColor">LightGray</Color>
        <Color x:Key="LightBevelBackgroundColor">LightGray</Color>
